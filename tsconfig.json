{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es2022"
    ],
    "module": "es2022",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "es2022"
  },
  "include": [
    "src/**/*.ts"
  ]
}
